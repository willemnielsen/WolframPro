Cell[BoxData[
 RowBox[{
  RowBox[{"ImportLife", "[", "files_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"check", ",", "res"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"check", "=", 
      RowBox[{"SelectFirst", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Reverse", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"\"\<https://conwaylife.com/patterns/\>\"", "<>", "#"}], 
           "&"}], "/@", "files"}], "\[IndentingNewLine]", "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"MatrixQ", "[", 
          RowBox[{"res", "=", 
           RowBox[{"Which", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"StringMatchQ", "[", 
              RowBox[{"#", ",", 
               RowBox[{"__", "~~", "\"\<.rle\>\""}]}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"ImportRLE", "[", "#", "]"}], ",", "\[IndentingNewLine]", 
             RowBox[{"StringMatchQ", "[", 
              RowBox[{"#", ",", 
               RowBox[{"__", "~~", "\"\<.cells\>\""}]}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"ImportCells", "[", "#", "]"}], ",", 
             "\[IndentingNewLine]", "True", ",", "\[IndentingNewLine]", 
             "$Failed"}], "\[IndentingNewLine]", "]"}]}], "]"}], "&"}], ",", 
        "\[IndentingNewLine]", "$Failed"}], "\[IndentingNewLine]", "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"FailureQ", "[", "check", "]"}], ",", "check", ",", "res"}], 
