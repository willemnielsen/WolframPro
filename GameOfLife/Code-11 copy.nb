             "___"}], "}"}], ",", "___"}], "}"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "$Failed", "]"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"rows", "=", 
      RowBox[{"ReplaceAll", "[", 
       RowBox[{
        RowBox[{"DeleteCases", "[", 
         RowBox[{"data", ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"\"\<!\>\"", ",", "___"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\<.\>\"", "->", "0"}], ",", 
