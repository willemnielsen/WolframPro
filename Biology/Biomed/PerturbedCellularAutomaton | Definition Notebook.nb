Cell[BoxData[
 RowBox[{
  RowBox[{"robustru", " ", "=", " ", 
   RowBox[{
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "ru", ",", " ", "ca", ",", " ", "lt", ",", " ", "pcas", ",", " ", 
        "fitness", ",", " ", 
        RowBox[{"testcalifetime", " ", "=", " ", 
         RowBox[{"Function", "[", 
          RowBox[{"ca", ",", " ", 
           RowBox[{
            RowBox[{
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"#", "==", "0"}], ",", " ", 
               RowBox[{"-", "Infinity"}], ",", " ", 
               RowBox[{
                RowBox[{"Length", "[", "ca", "]"}], "-", "#"}]}], "]"}], 
             "&"}], "[", 
            RowBox[{"LengthWhile", "[", 
